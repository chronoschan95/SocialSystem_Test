{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport Icon from '../components/common/Icon.vue';\nexport default {\n  name: 'Home',\n  components: {\n    Icon\n  },\n  setup() {\n    const currentTab = ref('privacy'); // 默认显示隐私事件记录\n    const showNav = ref(false);\n    const showSidebar = ref(false);\n    const newPost = ref('');\n    const tabs = [{\n      id: 'privacy',\n      name: '隐私记录'\n    }, {\n      id: 'posts',\n      name: '动态'\n    }, {\n      id: 'news',\n      name: '校园新闻'\n    }];\n    const toggleSidebar = () => {\n      showNav.value = !showNav.value;\n      showSidebar.value = !showSidebar.value;\n    };\n\n    // 模拟数据\n    const privacyEvents = ref([{\n      id: 1,\n      title: '咖啡配方记录',\n      version: '1.2',\n      content: '今天尝试了新的咖啡豆配比：巴西Santos 60% + 耶加雪菲 40%',\n      tags: ['咖啡', '配方', '记录']\n    }\n    // 更多事件...\n    ]);\n    return {\n      currentTab,\n      showNav,\n      showSidebar,\n      newPost,\n      tabs,\n      privacyEvents,\n      toggleSidebar\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","Icon","name","components","setup","currentTab","showNav","showSidebar","newPost","tabs","id","toggleSidebar","value","privacyEvents","title","version","content","tags"],"sources":["/Users/chronoschan/Documents/SophomoreYear/JAVA/ExperimentX/CSys/V1/frontend/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <!-- 顶部导航 -->\n    <header class=\"header glass-effect\">\n      <div class=\"logo-section\">\n        <img src=\"@/assets/logo.svg\" alt=\"Logo\" class=\"logo\" />\n        <h1>Social Platform</h1>\n      </div>\n      <div class=\"header-actions\">\n        <button class=\"theme-toggle\" @click=\"toggleTheme\">\n          <Icon :name=\"isDarkMode ? 'moon' : 'sun'\" />\n        </button>\n        <div class=\"user-avatar\" @click=\"toggleUserMenu\">\n          <img :src=\"userAvatar\" :alt=\"username\" />\n        </div>\n      </div>\n    </header>\n\n    <!-- 主要内容区 -->\n    <main class=\"main-content\">\n      <!-- 中心内容区 -->\n      <div class=\"content-area\">\n        <!-- 发布框 -->\n        <div class=\"post-composer glass-effect\">\n          <textarea \n            placeholder=\"分享你的想法...\"\n            v-model=\"newPost\"\n            @input=\"autoGrow\"\n            ref=\"postInput\"\n          ></textarea>\n          <div class=\"composer-actions\">\n            <div class=\"upload-buttons\">\n              <button class=\"upload-btn\">\n                <Icon name=\"image\" />\n              </button>\n              <button class=\"upload-btn\">\n                <Icon name=\"file\" />\n              </button>\n              <button class=\"upload-btn\">\n                <Icon name=\"tag\" />\n              </button>\n            </div>\n            <button class=\"post-btn\" :disabled=\"!newPost.trim()\">发布</button>\n          </div>\n        </div>\n\n        <!-- 内容标签页 -->\n        <div class=\"content-tabs glass-effect\">\n          <button \n            v-for=\"tab in tabs\" \n            :key=\"tab.id\"\n            :class=\"['tab-btn', { active: currentTab === tab.id }]\"\n            @click=\"currentTab = tab.id\"\n          >\n            {{ tab.name }}\n          </button>\n        </div>\n\n        <!-- 动态内容区 -->\n        <div class=\"feed-content\">\n          <!-- 隐私事件记录 -->\n          <div v-if=\"currentTab === 'privacy'\" class=\"privacy-events\">\n            <div class=\"event-card glass-effect\" v-for=\"event in privacyEvents\" :key=\"event.id\">\n              <div class=\"event-header\">\n                <h3>{{ event.title }}</h3>\n                <span class=\"version\">v{{ event.version }}</span>\n              </div>\n              <div class=\"event-content\">\n                <p>{{ event.content }}</p>\n                <div class=\"event-tags\">\n                  <span v-for=\"tag in event.tags\" :key=\"tag\" class=\"tag\">\n                    #{{ tag }}\n                  </span>\n                </div>\n              </div>\n              <div class=\"event-footer\">\n                <button class=\"history-btn\" @click=\"showHistory(event)\">\n                  <Icon name=\"history\" />\n                  历史版本\n                </button>\n                <button class=\"edit-btn\" @click=\"editEvent(event)\">\n                  <Icon name=\"edit\" />\n                  编辑\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <!-- 普通动态 -->\n          <div v-else-if=\"currentTab === 'posts'\" class=\"posts\">\n            <!-- 动态卡片组件 -->\n          </div>\n\n          <!-- 校园新闻 -->\n          <div v-else-if=\"currentTab === 'news'\" class=\"news\">\n            <!-- 新闻列表组件 -->\n          </div>\n        </div>\n      </div>\n\n      <!-- 右侧边栏 -->\n      <aside class=\"right-sidebar glass-effect\" :class=\"{ 'sidebar-expanded': showSidebar }\">\n        <div class=\"trending-topics\">\n          <h2>热门话题</h2>\n          <div class=\"topic-list\">\n            <div v-for=\"topic in trendingTopics\" :key=\"topic.id\" class=\"topic-item\">\n              <h3># {{ topic.name }}</h3>\n              <span class=\"topic-count\">{{ topic.count }}条讨论</span>\n            </div>\n          </div>\n        </div>\n      </aside>\n    </main>\n\n    <!-- 左下角菜单按钮 -->\n    <button class=\"menu-toggle\" @click=\"toggleSidebar\">\n      <Icon name=\"menu\" />\n    </button>\n\n    <!-- 模块导航菜单 -->\n    <nav class=\"module-nav glass-effect\" :class=\"{ 'nav-expanded': showNav }\">\n      <router-link to=\"/privacy\" class=\"nav-item\">\n        <Icon name=\"lock\" />\n        <span>隐私记录</span>\n      </router-link>\n      <router-link to=\"/posts\" class=\"nav-item\">\n        <Icon name=\"home\" />\n        <span>动态</span>\n      </router-link>\n      <router-link to=\"/messages\" class=\"nav-item\">\n        <Icon name=\"message\" />\n        <span>私信</span>\n      </router-link>\n      <router-link to=\"/notifications\" class=\"nav-item\">\n        <Icon name=\"notification\" />\n        <span>通知</span>\n      </router-link>\n    </nav>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from 'vue'\nimport Icon from '../components/common/Icon.vue'\n\nexport default {\n  name: 'Home',\n  components: { Icon },\n  \n  setup() {\n    const currentTab = ref('privacy') // 默认显示隐私事件记录\n    const showNav = ref(false)\n    const showSidebar = ref(false)\n    const newPost = ref('')\n\n    const tabs = [\n      { id: 'privacy', name: '隐私记录' },\n      { id: 'posts', name: '动态' },\n      { id: 'news', name: '校园新闻' }\n    ]\n\n    const toggleSidebar = () => {\n      showNav.value = !showNav.value\n      showSidebar.value = !showSidebar.value\n    }\n\n    // 模拟数据\n    const privacyEvents = ref([\n      {\n        id: 1,\n        title: '咖啡配方记录',\n        version: '1.2',\n        content: '今天尝试了新的咖啡豆配比：巴西Santos 60% + 耶加雪菲 40%',\n        tags: ['咖啡', '配方', '记录']\n      },\n      // 更多事件...\n    ])\n\n    return {\n      currentTab,\n      showNav,\n      showSidebar,\n      newPost,\n      tabs,\n      privacyEvents,\n      toggleSidebar\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.home {\n  min-height: 100vh;\n  background: var(--background-color);\n  \n  .header {\n    position: fixed;\n    top: 0;\n    left: 0;\n    right: 0;\n    height: 60px;\n    padding: 0 20px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    z-index: 100;\n  }\n\n  .main-content {\n    margin-top: 80px;\n    padding: 0 20px;\n    display: flex;\n    gap: 20px;\n    \n    .content-area {\n      flex: 1;\n      max-width: 800px;\n      margin: 0 auto;\n    }\n  }\n\n  .menu-toggle {\n    position: fixed;\n    left: 20px;\n    bottom: 20px;\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    background: var(--primary-color);\n    border: none;\n    color: white;\n    cursor: pointer;\n    z-index: 1000;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    \n    &:hover {\n      transform: scale(1.1);\n    }\n  }\n\n  .module-nav {\n    position: fixed;\n    left: 20px;\n    bottom: 80px;\n    padding: 10px;\n    border-radius: 12px;\n    transform: translateX(-120%);\n    transition: transform 0.3s ease;\n    \n    &.nav-expanded {\n      transform: translateX(0);\n    }\n    \n    .nav-item {\n      display: flex;\n      align-items: center;\n      gap: 10px;\n      padding: 12px;\n      color: var(--text-color);\n      text-decoration: none;\n      border-radius: 8px;\n      \n      &:hover {\n        background: rgba(255, 255, 255, 0.1);\n      }\n    }\n  }\n\n  .privacy-events {\n    .event-card {\n      margin-bottom: 20px;\n      padding: 20px;\n      border-radius: 12px;\n      \n      .event-header {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        margin-bottom: 12px;\n        \n        .version {\n          padding: 4px 8px;\n          background: var(--primary-color);\n          border-radius: 12px;\n          font-size: 12px;\n        }\n      }\n      \n      .event-tags {\n        display: flex;\n        gap: 8px;\n        margin-top: 12px;\n        \n        .tag {\n          padding: 4px 8px;\n          background: rgba(255, 255, 255, 0.1);\n          border-radius: 12px;\n          font-size: 12px;\n        }\n      }\n      \n      .event-footer {\n        display: flex;\n        justify-content: flex-end;\n        gap: 12px;\n        margin-top: 16px;\n        \n        button {\n          display: flex;\n          align-items: center;\n          gap: 6px;\n          padding: 8px 12px;\n          border: none;\n          border-radius: 8px;\n          cursor: pointer;\n          \n          &.history-btn {\n            background: rgba(255, 255, 255, 0.1);\n          }\n          \n          &.edit-btn {\n            background: var(--primary-color);\n            color: white;\n          }\n        }\n      }\n    }\n  }\n}\n\n.glass-effect {\n  background: rgba(255, 255, 255, 0.1);\n  backdrop-filter: blur(10px);\n  border: 1px solid rgba(255, 255, 255, 0.2);\n}\n</style> "],"mappings":"AA8IA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAI;AACnC,OAAOC,IAAG,MAAO,+BAA8B;AAE/C,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEF;EAAK,CAAC;EAEpBG,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIN,GAAG,CAAC,SAAS,GAAE;IAClC,MAAMO,OAAM,GAAIP,GAAG,CAAC,KAAK;IACzB,MAAMQ,WAAU,GAAIR,GAAG,CAAC,KAAK;IAC7B,MAAMS,OAAM,GAAIT,GAAG,CAAC,EAAE;IAEtB,MAAMU,IAAG,GAAI,CACX;MAAEC,EAAE,EAAE,SAAS;MAAER,IAAI,EAAE;IAAO,CAAC,EAC/B;MAAEQ,EAAE,EAAE,OAAO;MAAER,IAAI,EAAE;IAAK,CAAC,EAC3B;MAAEQ,EAAE,EAAE,MAAM;MAAER,IAAI,EAAE;IAAO,EAC7B;IAEA,MAAMS,aAAY,GAAIA,CAAA,KAAM;MAC1BL,OAAO,CAACM,KAAI,GAAI,CAACN,OAAO,CAACM,KAAI;MAC7BL,WAAW,CAACK,KAAI,GAAI,CAACL,WAAW,CAACK,KAAI;IACvC;;IAEA;IACA,MAAMC,aAAY,GAAId,GAAG,CAAC,CACxB;MACEW,EAAE,EAAE,CAAC;MACLI,KAAK,EAAE,QAAQ;MACfC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,sCAAsC;MAC/CC,IAAI,EAAE,CAAC,IAAI,EAAE,IAAI,EAAE,IAAI;IACzB;IACA;IAAA,CACD;IAED,OAAO;MACLZ,UAAU;MACVC,OAAO;MACPC,WAAW;MACXC,OAAO;MACPC,IAAI;MACJI,aAAa;MACbF;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}